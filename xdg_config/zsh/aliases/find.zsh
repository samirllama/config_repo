# Find command aliases
alias flf="find . -size +10M"  # Find files larger than 10MB
alias frcf="find . -mtime -1"  # Find files modified in the last 24 hours
alias fpyf="find . -name '*.py'"  # Find Python files

# Grep command aliases
alias grep_r="grep -r"  # Recursive search
alias grep_c="grep -c"  # Count occurrences
alias grep_i="grep -v"  # Invert match